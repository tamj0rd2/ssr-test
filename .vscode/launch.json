  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch via NPM",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run-script", "dev"],
      "port": 9229,
      // "preLaunchTask": "Build dev",
      "sourceMaps": true,
      "skipFiles": ["<node_internals>/**/*.js", "${workspaceFolder}/node_modules/**/*.js"],
      "console": "integratedTerminal"
    }
  ]
}
